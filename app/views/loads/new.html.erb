<div class='loadinfo'>  
        <%= form_for(@load,  :html => {:class => "load form-horizontal center"} ) do |f| %>  
                <div class="form-group">
                    <%= f.label :email %>
                    <%= f.text_field :email, class: "form-control" %>
                </div>
                <div class="form-group">
                    <%= f.label :file, "NCBI annotation pipeline file"%>
                    <%= f.file_field :file, class: "form-control"  %>
                </div>
                <%= f.submit "load", class: "btn " %>
        <% end %>
</div>

 